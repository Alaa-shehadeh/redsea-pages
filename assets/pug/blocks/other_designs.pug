section.section.sectionBgTrigger(data-id-color="3")
	.section_in
		.other_wrap
			.other_col.sm_mod
				.section_head.decor_mod.fadeEl
					h2.section_title.offset_v2_mod #[span.title_in= data.sheybarahPage.other.pretitle]
						= data.sheybarahPage.other.title
					.section_descr= data.sheybarahPage.other.descr 
			.other_col.lg_mod
				.other_slider_wrap
					+slider_controls({
						mod: 'v2_mod other_mod fadeEl',
						icon_mod: 'arrow_mod'
					})
					.other_slider
						each item in data.sheybarahPage.other.list
							+other(item)
